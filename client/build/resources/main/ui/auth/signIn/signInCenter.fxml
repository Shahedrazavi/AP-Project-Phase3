<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.auth.signIn.SignInCenterFXMLController">
   <children>
      <Label fx:id="signInLabel" layoutX="473.0" layoutY="150.0" text="Sign In">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="signInButton" layoutX="474.0" layoutY="530.0" mnemonicParsing="false" onAction="#signInPressed" text="Sign In" />
      <TextField fx:id="usernameBox" layoutX="324.0" layoutY="280.0" promptText="Username" />
      <TextField fx:id="passwordBox" layoutX="324.0" layoutY="380.0" promptText="Password" />
      <Label fx:id="errorLabel" layoutX="528.0" layoutY="330.0" text="Invalid username &amp; password. Please try again." textFill="RED" />
      <AnchorPane fx:id="backButton" layoutX="14.0" layoutY="14.0" prefHeight="25.0" prefWidth="60.0" />
   </children>
</AnchorPane>
